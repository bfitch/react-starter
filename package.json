{
  "name": "react-starter",
  "version": "1.0.0",
  "description": "Starter for React",
  "main": "",
  "scripts": {
    "dev": "webpack-dev-server --hot --devtool source-map --progress --colors --inline --history-api-fallback",
    "server-watch": "./node_modules/.bin/babel api -d dist --experimental -w -s",
    "server": "nodemon dist/server.js",
    "server-dev": "npm run server-watch & npm run server",
    "start": "npm run server-dev & npm run dev"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "react": "^0.13.3",
    "react-router": "^0.13.3"
  },
  "devDependencies": {
    "axios": "^0.5.4",
    "babel": "^5.6.23",
    "babel-core": "^5.4.7",
    "babel-eslint": "^3.1.15",
    "babel-loader": "^5.1.3",
    "babel-runtime": "^5.6.15",
    "baobab": "^1.1.1",
    "baobab-react": "^0.1.1",
    "body-parser": "^1.13.2",
    "cors": "^2.7.1",
    "eslint": "^0.21.2",
    "eslint-loader": "^0.11.2",
    "eslint-plugin-react": "^2.5.0",
    "express": "^4.13.1",
    "json-loader": "^0.5.2",
    "morgan": "^1.6.1",
    "node-libs-browser": "^0.5.2",
    "nodemon": "^1.3.7",
    "webpack": "^1.9.10",
    "webpack-dev-server": "^1.9.0"
  },
  "eslintConfig": {
    "env": {
      "browser": true,
      "node": true,
      "jquery": true,
      "es6": true
    },
    "ecmaFeatures": {
      "jsx": true
    },
    "plugins": [
      "react"
    ],
    "parser": "babel-eslint",
    "rules": {
      "camelcase": 0,
      "quotes": [
        1,
        "double",
        "avoid-escape"
      ],
      "no-unused-vars": 0,
      "new-cap": 1,
      "no-console": 0,
      "no-debugger": 0,
      "no-undef": 2,
      "no-var": 2,
      "no-multi-spaces": 0,
      "strict": [
        0,
        "never"
      ],
      "react/display-name": 0,
      "react/jsx-boolean-value": 1,
      "react/jsx-no-undef": 1,
      "react/jsx-quotes": 1,
      "react/jsx-sort-prop-types": 1,
      "react/jsx-sort-props": 0,
      "react/jsx-uses-react": 1,
      "react/jsx-uses-vars": 1,
      "react/no-did-mount-set-state": 1,
      "react/no-did-update-set-state": 1,
      "react/no-multi-comp": 0,
      "react/no-unknown-property": 1,
      "react/prop-types": 0,
      "react/react-in-jsx-scope": 1,
      "react/self-closing-comp": 1,
      "react/sort-comp": 1,
      "react/wrap-multilines": 1
    }
  }
}
